{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/about/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog/?$",
			"dest": "_render"
		},
		{
			"src": "^/companies/?$",
			"dest": "_render"
		},
		{
			"src": "^/news/?$",
			"dest": "_render"
		},
		{
			"src": "^/registration-types/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/registration-types/?$",
			"dest": "_render"
		},
		{
			"src": "^/search(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/sitemap\\.xml/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}